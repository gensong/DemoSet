        <service
            android:name="com.demo.song.service.service.MyService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name="com.demo.song.service.service.MyIntentService"
            android:exported="false" />

        <activity android:name="com.demo.song.animate.MyAnimateListActivity" />
        <activity android:name="com.demo.song.animate.AnimateActivity" />
